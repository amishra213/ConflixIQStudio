{
  "version": "1.0.0",
  "source": "./index.html",
  "type": "module",
  "name": "conflixiq-studio",
  "description": "ConflixIQ Studio - A comprehensive visual workflow designer for Netflix Conductor",
  "author": "ConflixIQ Studio Team",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "server": "node index.js",
    "server:dev": "node --watch index.js",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "docker-build": "node -e \"require('fs').mkdirSync('dist/release',{recursive:true})\" && docker build -t conflixiq-studio:latest . && docker save -o dist/release/conflixiq-studio-latest.tar conflixiq-studio:latest",
    "docker-create": "docker create --name conflixiq-studio -p 4000:4000 conflixiq-studio:latest",
    "docker-run": "docker run -d --name conflixiq-studio -p 4000:4000 conflixiq-studio:latest",
    "docker-start": "docker start conflixiq-studio",
    "docker-stop": "docker stop conflixiq-studio",
    "docker-rm": "docker rm conflixiq-studio",
    "docker-export": "node -e \"require('fs').mkdirSync('dist/release',{recursive:true})\" && docker save -o dist/release/conflixiq-studio-latest.tar conflixiq-studio:latest",
    "docker-export:gz": "node -e \"require('fs').mkdirSync('dist/release',{recursive:true})\" && docker save conflixiq-studio:latest | gzip > dist/release/conflixiq-studio-latest.tar.gz",
    "docker-compose": "docker compose up -d",
    "docker-compose:down": "docker compose down",
    "docker-compose:logs": "docker compose logs -f",
    "sea:build": "tsx scripts/sea-builder.ts all",
    "sea:build:windows": "tsx scripts/sea-builder.ts windows",
    "sea:build:windows:fast": "tsx scripts/sea-builder.ts windows --skip-ui-build",
    "sea:build:docker": "tsx scripts/sea-builder.ts docker"
  },
  "dependencies": {
    "@apollo/client": "^4.0.9",
    "@apollo/server": "^4.10.1",
    "@radix-ui/react-accordion": "1.2.1",
    "@radix-ui/react-alert-dialog": "1.1.2",
    "@radix-ui/react-aspect-ratio": "1.1.0",
    "@radix-ui/react-avatar": "1.1.1",
    "@radix-ui/react-checkbox": "1.1.2",
    "@radix-ui/react-collapsible": "1.1.1",
    "@radix-ui/react-context-menu": "2.2.2",
    "@radix-ui/react-dialog": "1.1.2",
    "@radix-ui/react-dropdown-menu": "2.1.2",
    "@radix-ui/react-hover-card": "1.1.2",
    "@radix-ui/react-label": "2.1.0",
    "@radix-ui/react-menubar": "1.1.2",
    "@radix-ui/react-navigation-menu": "1.2.1",
    "@radix-ui/react-popover": "1.1.2",
    "@radix-ui/react-progress": "1.1.0",
    "@radix-ui/react-radio-group": "1.2.1",
    "@radix-ui/react-scroll-area": "1.2.0",
    "@radix-ui/react-select": "2.1.2",
    "@radix-ui/react-separator": "1.1.0",
    "@radix-ui/react-slider": "1.2.1",
    "@radix-ui/react-slot": "1.1.0",
    "@radix-ui/react-switch": "1.1.1",
    "@radix-ui/react-tabs": "1.1.1",
    "@radix-ui/react-toast": "1.2.1",
    "@radix-ui/react-toggle": "1.1.0",
    "@radix-ui/react-toggle-group": "1.1.0",
    "@radix-ui/react-tooltip": "1.1.3",
    "@tanstack/react-query": "^5.90.2",
    "axios": "^1.6.0",
    "class-variance-authority": "0.7.0",
    "clsx": "^2.0.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "graphql-type-json": "^0.3.2",
    "gsap": "^3.13.0",
    "lucide-react": "^0.544.0",
    "mermaid": "^10.6.1",
    "open": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "reactflow": "^11.11.4",
    "recharts": "^2.10.0",
    "tailwind-merge": "^1.14.0",
    "uuid": "^10.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/gsap": "^1.20.2",
    "@types/node": "^20.14.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "15.12.0",
    "postject": "^1.0.0-alpha.6",
    "prettier": "^3.2.5",
    "tailwindcss": "3.4.16",
    "tailwindcss-animate": "1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^6.4.1"
  },
  "overrides": {
    "glob": "12.0.0"
  },
  "alias": {
    "@/*": "./src/components/ui/$1"
  }
}
